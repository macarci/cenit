-#rails_admin-1.0 ready
- unless @object.try(:ready_to_save?).nil?
  =render partial: 'wizard_steps'
= rails_admin_form_for @object, url: new_path(model_name: @abstract_model.to_param, contextual_params: params[:contextual_params]), as: @abstract_model.param_key, html: { multipart: true, class: "form-horizontal denser", data: { title: @page_name } } do |form|
  = form.generate action: :create
